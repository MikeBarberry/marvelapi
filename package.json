{
  "name": "marvelcharacters-next",
  "version": "2.0",
  "author": "Mike Barberry",
  "license": "GPL-3.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "buildprod": "CONFIG_ENV=prod npm run fullbuild",
    "fullbuild": "cfn-lint template.yaml && npm run docker:login && npm run docker:build && npm run docker:push && npm run sam",
    "docker:login": "set-env -e -- 'aws ecr get-login-password --region us-west-2 | docker login --username AWS --password-stdin $ECR_ID.dkr.ecr.us-west-2.amazonaws.com'",
    "docker:build": "set-env -e -u -- 'docker build -t $ECR_ID.dkr.ecr.us-west-2.amazonaws.com/marvelcharacters:$BUILD_ID .'",
    "docker:push": "set-env -e -i -- 'docker push $ECR_ID.dkr.ecr.us-west-2.amazonaws.com/marvelcharacters:$BUILD_ID'",
    "sam": "sam build && sam deploy --config-env=prod"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.14.11",
    "@mui/lab": "^5.0.0-alpha.113",
    "@mui/material": "^5.11.1",
    "@next/font": "13.0.7",
    "@vendia/serverless-express": "^4.10.1",
    "eslint-config-next": "13.0.7",
    "express": "^4.18.2",
    "mongodb": "^4.12.1",
    "next": "13.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-loader-spinner": "^5.3.4",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "eslint": "^8.30.0",
    "set-env": "file:utils/set-env-1.3.tgz"
  }
}
